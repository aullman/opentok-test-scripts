{
  "name": "opentok-test-scripts",
  "version": "3.5.1",
  "description": "Testing scripts for OpenTok applications",
  "main": "run-tests",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "./run-tests",
    "start": "http-server -p 5000",
    "unit": "karma start tests/karma.conf.js --single-run",
    "integration": "webdriver-manager update && protractor-flake -- tests/protractor.conf.js"
  },
  "bin": {
    "run-tests": "./run-tests",
    "sauceconnect-start": "./sauceconnect-start",
    "sauceconnect-stop": "./sauceconnect-stop",
    "browserstacklocal-start": "./browserstacklocal-start",
    "browserstacklocal-stop": "./browserstacklocal-stop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aullman/opentok-test-scripts.git"
  },
  "keywords": [
    "karma",
    "protractor",
    "travis",
    "opentok",
    "webrtc",
    "saucelabs",
    "browserstack",
    "webdriver"
  ],
  "author": "Adam Ullman <adam@tokbox.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aullman/opentok-test-scripts/issues"
  },
  "homepage": "https://github.com/aullman/opentok-test-scripts#readme",
  "dependencies": {
    "travis-multirunner": "^4.2.0"
  },
  "devDependencies": {
    "firefox-profile": "^0.3.11",
    "http-server": "^0.8.5",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.15",
    "karma-chrome-launcher": "^0.2.1",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.6",
    "karma-safari-launcher": "^1.0.0",
    "karma-safaritechpreview-launcher": "0.0.6",
    "karma-sauce-launcher": "^0.3.0",
    "protractor": "^4.0.9",
    "protractor-flake": "^2.5.1",
    "q": "^1.4.1"
  }
}
